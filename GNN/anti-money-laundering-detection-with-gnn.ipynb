    "        ax.text(bar.get_x() + bar.get_width() / 2, bar.get_height(), f'{count:,}',\n",
    "                ha='center', va='bottom', fontsize=11, fontweight='bold')\n",
    "\n",
    "    total = counts.sum()\n",
    "    ax.set_title(title, fontsize=13, fontweight='bold')\n",
